	option enabled '0'
	option input 'uvc'
	option output 'http'
	option device '/dev/video0'
	option resolution '1280x720'
	option yuv '0'
	option quality '80'
	option fps '10'
	option led 'auto'
	option www '/www/webcam'
	option port '8080'
	#option listen_ip '192.168.1.1'
	#option username 'openwrt'
	#option password 'openwrt'
